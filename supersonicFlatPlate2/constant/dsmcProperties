{
    Tref                                       290;
}


// Collision Partner Selection Model
// ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

collisionPartnerSelectionModel   		noTimeCounter;  


// Molecular species
// ~~~~~~~~~~~~~~~~~

typeIdList                      (N2cold);

moleculeProperties
{
